{
  "name": "@minimal/material-kit-react",
  "author": "Minimals UI",
  "version": "4.0.0",
  "description": "Simple React Scripts & Typescript",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "test:e2e": "npx playwright test --config=playwright/playwright.config.ts --ui",
    "test:e2e:headless": "npx playwright test --config=playwright/playwright.config.ts",
    "test:e2e:report": "yarn playwright show-report",
    "test:e2e:debug": "yarn playwright  test --config=playwright/playwright.config.ts --debug"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
